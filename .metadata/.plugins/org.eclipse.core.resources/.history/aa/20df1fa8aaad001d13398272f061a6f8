package org;

public class FirstExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// Single line comment
		/*
		 * Block comment
		 * write a program to add two numbers
		 */
		int a,b,res;
		a=100;
		b=200;
		res=a+b;
		System.out.println("Addition of two number is -> "+res);
	}

} 
